{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src/"],
    "includePatter": ".js$",
    "excludePatter": "(node_modules/|docs)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks" : false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./out/",
    "recurse": true
  }
}